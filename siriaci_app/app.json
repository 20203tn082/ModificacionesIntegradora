{
  "expo": {
    "name": "siriaci",
    "slug": "siriaci",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icono.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "mx.edu.utez.siriaci_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Version 1.0 de siriaci app m√≥vil",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icono.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
